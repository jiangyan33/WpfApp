<Window x:Class="WpfApp10.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp10"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <Style TargetType="Button">
            <Setter Property="ItemsControl.Margin" Value="5" />
            <Setter Property="ItemsControl.Padding" Value="5" />
            <Setter Property="FontSize" Value="18" />
            <Setter Property="FontFamily" Value="微软雅黑" />
        </Style>
    </Window.Resources>
    <StackPanel>
        <Button Content="变换与透明" Name="button1" Click="button1_Click"/>
        <Button Content="路径和集合图形" Name="button2" Click="button2_Click"/>
        <Button Content="测试集合图形" Name="button3" Click="button3_Click"/>
        <Button Content="可视化对象" Name="button4" Click="button4_Click"/>
        <Button Content="Button5" Name="button5"/>
        <Button Content="Button6" Name="button6"/>
        <Button Content="Button7" Name="button7"/>
        <Button Content="Button8" Name="button8"/>
        <Button Content="Button9" Name="button9"/>
    </StackPanel>
</Window>
